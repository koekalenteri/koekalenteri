    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/aws-lambda": "^8.10.150",
    "@types/busboy": "^1.5.4",
    "@types/css-mediaquery": "^0.1.2",
    "@types/hast": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.15.32",
