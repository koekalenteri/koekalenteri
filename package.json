{
  "name": "koekalenteri",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install": "npm run install-be && npm run install-fe",
    "install-be": "cd lambda && npm ci",
    "install-fe": "cd web && npm ci",
    "docker-init": "docker network create lambda-local",
    "dynamodb": "docker run -d --name dynamodb --network lambda-local -p 8000:8000 amazon/dynamodb-local",
    "start": "shx rm -rf .aws-sam && concurrently npm:start-*",
    "start-dynamodb": "docker start dynamodb && docker attach dynamodb",
    "start-labmda": "cd lambda && npm start",
    "start-sam": "sam local start-api -p 8080 --docker-network lambda-local --warm-containers=LAZY",
    "start-web": "cd web && npm start"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "shx": "^0.3.4"
  }
}
