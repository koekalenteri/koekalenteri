    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "babel-preset-react-app": "^10.1.0",
    "camelcase": "^8.0.0",
    "concurrently": "^9.1.2",
    "core-js": "^3.43.0",
    "css-loader": "^7.1.2",
    "css-mediaquery": "^0.1.2",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.4",
