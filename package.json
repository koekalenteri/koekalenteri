    "source-map-explorer": "^2.5.3",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "tunnelmole": "^2.4.0",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3",
    "webpack": "^5.99.9",
    "webpack-dev-server": "^5.2.1",
    "webpack-manifest-plugin": "^5.0.1",
    "workbox-webpack-plugin": "^7.3.0"
  },
