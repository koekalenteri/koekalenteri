{
  "name": "koekalenteri-backend",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --silent",
    "test:log": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "prebuild": "node copy_i18n.mjs",
    "build": "node build.js",
    "prewatch": "node copy_i18n.mjs",
    "watch": "node build.js --watch",
    "prelint": "node copy_i18n.mjs",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "lint:tsc": "tsc --noEmit",
    "create-tables": "run-script-os",
    "create-tables:nix": "./__test__/environment/local/create_tables.sh",
    "create-tables:win32": "cd ./__test__/environment/local/ && @powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./create_tables.ps1",
    "start": "npm run create-tables && npm run watch"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "aws-embedded-metrics": "^4.0.0",
    "aws-sdk": "^2.1478.0",
    "busboy": "^1.6.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "deep-object-diff": "^1.1.9",
    "i18next": "^23.6.0",
    "nanoid": "^4.0.2",
    "node-fetch": "^3.3.2",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.1",
    "source-map-support": "^0.5.19",
    "unified": "^10.1.2",
    "unist-builder": "^4.0.0",
    "unist-util-position": "^5.0.0",
    "unist-util-visit": "^5.0.0",
    "uuid": "^9.0.1",
    "vfile-reporter": "^8.1.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "@types/aws-lambda": "^8.10.125",
    "@types/busboy": "^1.5.2",
    "@types/hast": "^3.0.2",
    "@types/jest": "^29.5.7",
    "@types/mdast": "^4.0.2",
    "@types/node": "^20.8.7",
    "@types/node-fetch": "^2.6.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fs-extra": "^11.1.0",
    "jest": "^29.7.0",
    "koekalenteri-shared": "file:../koekalenteri-shared",
    "mdast-util-to-hast": "^13.0.2",
    "prettier": "^3.0.3",
    "run-script-os": "^1.1.6",
    "to-vfile": "^8.0.0",
    "typescript": "^5.2.2"
  }
}
