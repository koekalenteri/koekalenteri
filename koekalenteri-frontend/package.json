{
  "name": "koekalenteri-frontend",
  "version": "0.5.2",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^5.1.0",
    "@aws-amplify/ui-react": "^4.2.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.0.0",
    "@mui/x-data-grid": "^5.2.1",
    "@mui/x-date-pickers": "^5.0.3",
    "aws-amplify": "^5.0.6",
    "date-fns": "^2.23.0",
    "i18next": "^22.4.5",
    "i18next-browser-languagedetector": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "material-ui-confirm": "^3.0.7",
    "notistack": "^2.0.3",
    "preval.macro": "^5.0.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.0",
    "recoil": "^0.7.6",
    "recursive-diff": "^1.0.8",
    "source-map-explorer": "^2.5.2",
    "uuid": "^9.0.0",
    "web-vitals": "^3.1.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "describe-stacks": "aws cloudformation describe-stacks --stack-name koekalenteri-dev --query Stacks[0] > stack.json",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint \"src/**/*{.ts,.tsx}\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react",
      "simple-import-sort"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "@typescript-eslint/indent": [
        "error",
        2
      ],
      "simple-import-sort/imports": [
        "error",
        {
          "groups": [
            [
              "^react",
              "^@?\\w"
            ],
            [
              "^(@|components)(/.*|$)"
            ],
            [
              "^\\u0000"
            ],
            [
              "^\\.\\.(?!/?$)",
              "^\\.\\./?$"
            ],
            [
              "^\\./(?=.*/)(?!/?$)",
              "^\\.(?!/?$)",
              "^\\./?$"
            ],
            [
              "^.+\\.?(css)$"
            ]
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/build/",
      "/node_modules/",
      "/index.tsx",
      "/reportWebVitals.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-dnd|dnd-core|@react-dnd|react-dnd-html5-backend)/)"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/css-mediaquery": "^0.1.1",
    "@types/jest": "^29.2.4",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^18.11.15",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "core-js": "^3.27.1",
    "css-mediaquery": "^0.1.2",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest-fetch-mock": "^3.0.3",
    "koekalenteri-shared": "file:../koekalenteri-shared",
    "typescript": "^4.6.3"
  }
}
