{
  "name": "koekalenteri-frontend",
  "version": "0.3.3",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^5.1.0",
    "@aws-amplify/ui-react": "^4.2.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.0.0",
    "@mui/x-data-grid": "^5.2.1",
    "@mui/x-date-pickers": "^5.0.3",
    "aws-amplify": "^5.0.6",
    "date-fns": "^2.23.0",
    "history": "^5.0.1",
    "i18next": "^22.4.5",
    "i18next-browser-languagedetector": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "koekalenteri-shared": "file:../koekalenteri-shared",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "notistack": "^2.0.3",
    "preval.macro": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.0",
    "source-map-explorer": "^2.5.2",
    "uuid": "^9.0.0",
    "web-vitals": "^3.1.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "describe-stacks": "aws cloudformation describe-stacks --stack-name koekalenteri-dev --query Stacks[0] > stack.json",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint \"src/**/*{.ts,.tsx}\"",
    "test": "react-scripts test --env=jsdom --silent",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/indent": [
        "error",
        2
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/build/",
      "/node_modules/",
      "/index.tsx",
      "/reportWebVitals.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^18.11.15",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "eslint-config-react-app": "^7.0.0",
    "jest-fetch-mock": "^3.0.3",
    "typescript": "^4.6.3"
  }
}
